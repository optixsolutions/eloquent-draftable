{
    "name": "optix/eloquent-draftable",
    "description": "Add draftable functionality to your eloquent models.",
    "license": "MIT",
    "authors": [
        {
            "name": "Jack Robertson",
            "email": "jack@optixsolutions.co.uk"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/database": "~5.8.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.8.0|^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Optix\\Draftable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Optix\\Draftable\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
